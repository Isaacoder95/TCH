<?xml version='1.0' encoding='utf-8'?>
<widget id="com.isaac.TCH" version="1.1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>TCH</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="KeepRunning" value="true" />
    <preference name="Orientation" value="portrait" />
    <preference name="SplashScreenDelay" value="1000" />
    <preference name="Fullscreen" value="false" />
    <preference name="StatusBarBackgroundColor" value="#272C30" />
    <preference name="HeaderColor" value="#272C30" />
</widget>
